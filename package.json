    "@mui/x-date-pickers": "^6.18.6",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-slick": "^0.23.13",
    "date-fns": "^2.30.0",
    "firebase": "^11.4.0",
    "i18next": "^24.2.2",
    "lottie-react": "^2.4.1",
    "next": "15.2.2",
    "react": "^19.0.0",
    "react-datepicker": "^8.2.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
