    "@types/stylis": "^4.2.7",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "gh-pages": "^6.3.0",
    "json-loader": "^0.5.7",
    "tailwindcss": "^4",
    "typescript": "^5"
